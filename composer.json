        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "yiisoft/yii2-imagine": "*",
        "consynki/yii2-image-input": "*",
        "wbraganca/yii2-dynamicform": "dev-master",
        "mihaildev/yii2-ckeditor": "*",
        "ps/image-optimizer": "^1.1",
